<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_rentcar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_rentcar">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imageButton_rentnow"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/rentcar_rentnow_bar" />

        <ImageButton
            android:id="@+id/imageButton_carplat"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/rentcar_carplat_bar" />

        <ImageButton
            android:id="@+id/imageButton_carmoa"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/rentcar_carmoa_bar" />

        <ImageButton
            android:id="@+id/imageButton_ppongcar"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/rentcar_ppongcar_bar" />

        <Button
            android:id="@+id/blank4"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:alpha="1"
            android:background="#f7f8f8" />
    </LinearLayout>
</FrameLayout>