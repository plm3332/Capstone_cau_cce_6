<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_kickboard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_kickboard">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imageButton_kickgong"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/kickboard_kickgong_bar" />

        <ImageButton
            android:id="@+id/imageButton_gbility2"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/kickboard_gbility_bar" />

        <ImageButton
            android:id="@+id/imageButton_alpaca"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/kickboard_alpaca_bar" />

        <Button
            android:id="@+id/blank3"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:alpha="1"
            android:background="#f7f8f8" />
    </LinearLayout>

</FrameLayout>