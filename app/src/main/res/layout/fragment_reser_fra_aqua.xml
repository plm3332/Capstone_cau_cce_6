<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_carchap"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_aqua">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <Spinner
                    android:id="@+id/spinner"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:entries="@array/carchap_aqua_start_point_array"
                    android:gravity="left" />

                <Spinner
                    android:id="@+id/spinner2"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignTop="@+id/spinner"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:entries="@array/carchap_aqua_finish_point_array"
                    android:gravity="right" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/imageButton_aqua_aquabar"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@null"
                android:scaleType="fitXY"
                android:src="@drawable/aqua_aquabar" />

        </LinearLayout>

    </FrameLayout>

    <!-- TODO: Update blank fragment layout -->

</FrameLayout>