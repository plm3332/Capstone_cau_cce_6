<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_carchap"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_aqua">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="165dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/aqua_aquabar" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="50dp"
            android:entries="@array/carchap_aqua_start_point_array"
            android:gravity="left" />

        <Spinner
            android:id="@+id/spinner2"
            android:layout_width="177dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/spinner"
            android:entries="@array/carchap_aqua_finish_point_array"
            android:gravity="right" />

    </RelativeLayout>

    <!-- TODO: Update blank fragment layout -->

</FrameLayout>