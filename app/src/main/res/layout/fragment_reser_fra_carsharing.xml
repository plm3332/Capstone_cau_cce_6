<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_carsharing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_carsharing">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imageButton_socar"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carsharing_socar_bar" />

        <ImageButton
            android:id="@+id/imageButton_greencar"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carsharing_greencar_bar" />

        <ImageButton
            android:id="@+id/imageButton_dealcar"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carsharing_dealcar_bar" />

        <ImageButton
            android:id="@+id/imageButton_piplecar"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carsharing_pipolecar_bar" />

    </LinearLayout>
</FrameLayout>