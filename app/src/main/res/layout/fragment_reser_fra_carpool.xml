<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_reser_carpool"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserFra_carpool">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imageButton_tada"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carpool_tada_bar" />

        <ImageButton
            android:id="@+id/imageButton_buxi"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carpool_buxi_bar" />

        <ImageButton
            android:id="@+id/imageButton_poolus"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carpool_poolus_bar" />

        <ImageButton
            android:id="@+id/imageButton_uber"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carpool_uber_bar" />

        <ImageButton
            android:id="@+id/imageButton_adigo"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@null"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/carpool_adigo_bar" />

    </LinearLayout>

</FrameLayout>